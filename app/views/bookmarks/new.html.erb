<h2>Add movie to <%= @list.name %></h2>

<div class="row">
  <div class="col-6">
    <%= simple_form_for [@list, @bookmark] do |f|%>
      <%= f.association :movie, value_method: :id %>
      <%= f.input :comment %>
      <%= f.button :submit, "Add movie", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
